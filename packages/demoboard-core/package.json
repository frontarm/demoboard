{
  "name": "@frontarm/demoboard-core",
  "version": "0.1.7",
  "description": "Embeddable code editor",
  "author": "James K Nelson <james@jamesknelson.com>",
  "license": "Apache-2.0",
  "module": "dist/es/index.js",
  "browser": {
    "worker-loader!@frontarm/demoboard-worker": "./dist/es/demoboardWorker.fallback.js"
  },
  "types": "dist/types/index.d.ts",
  "scripts": {
    "clean": "rimraf dist/**",
    "build": "cross-env NODE_ENV=development rollup -c",
    "build:production": "cross-env NODE_ENV=production rollup -c",
    "build:watch": "yarn run build --watch",
    "lint": "eslint --ext js,ts,tsx src",
    "prepare": "yarn run clean && yarn run build:production",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@frontarm/demoboard-messaging": "^0.1.3",
    "@frontarm/demoboard-runtime": "^0.1.3",
    "@frontarm/demoboard-worker": "^0.1.6",
    "automerge": "^0.12.1",
    "comlink": "^4.0.5",
    "exenv": "^1.2.2",
    "file-loader": "^2.0.0",
    "file-saver": "^2.0.0",
    "jszip": "^3.1.5",
    "react-hot-keys": "^1.2.2",
    "react-visibility-sensor": "^4.1.2",
    "resize-observer-polyfill": "^1.5.0",
    "source-map": "^0.7.3",
    "tslib": "1.10.0",
    "worker-loader": "2.0.0"
  },
  "peerDependencies": {
    "@mdx-js/mdx": "^1.4.5",
    "@types/codemirror": "^0.0.77",
    "@types/exenv": "^1.2.0",
    "@types/jszip": "^3.1.6",
    "@types/lru-cache": "^5.1.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "5fb390c4abd17afc3e5c7eb644f1aa26a451888d"
