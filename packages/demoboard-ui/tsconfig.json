{
  "compilerOptions": {
      "declaration": true,
      "declarationDir": "dist/types",
      "downlevelIteration": true,
      "esModuleInterop": true,
      "jsx": "preserve",
      "target": "es5",
      "module": "esnext",
      "moduleResolution": "node",
      "outDir": "dist/es",
      "removeComments": false,
      "strict": true,
      "strictPropertyInitialization": false,
      "sourceMap": true,
      "lib": [
          "es5",
          "es6",
          "dom",
          "dom.iterable",
          "es2015",
          "es2016",
          "es2017",
          "webworker"
      ]
  },
  "exclude": [
      "dist",
      "node_modules",
      "test"
  ]
}