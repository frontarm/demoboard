{
  "name": "@frontarm/demoboard-runtime",
  "version": "0.1.1",
  "description": "Embeddable code editor runtime",
  "author": "James K Nelson <james@jamesknelson.com>",
  "license": "Apache 2.0",
  "main": "dist/demoboard-runtime.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build:umd": "cross-env NODE_ENV=development rollup -c -o dist/demoboard-runtime.js",
    "build:umd:min": "cross-env NODE_ENV=production rollup -c -o dist/demoboard-runtime.min.js",
    "build": "yarn run clean && yarn run build:umd && yarn run build:umd:min && cp src/container.html dist/container.html",
    "build:watch": "yarn run build:umd -- --watch",
    "prepublish": "yarn run build"
  },
  "dependencies": {
    "@frontarm/demoboard-messaging": "^0.1.1",
    "polestar": "^0.1.2",
    "regenerator-runtime": "^0.12.1",
    "replicator": "^1.0.2"
  }
}
